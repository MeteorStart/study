.my-setting {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 690rpx;
    height: 120rpx;
    background: #FFFFFF;
    justify-content: space-between;
    border-radius: 20rpx 20rpx 20rpx 20rpx;
}

.my-setting-icon {
    width: 30rpx;
    height: 30rpx;
    margin-left: 30rpx;
}

.my-setting-title {
    font-size: 30rpx;
    flex-grow: 1;
    margin-left: 20rpx;
    font-weight: 400;
    color: #666666;
}

.my-setting-enter {
    width: 14rpx;
    height: 24rpx;
    margin-right: 20rpx;
}